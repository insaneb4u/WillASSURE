
@{
    ViewBag.Title = "DisplayAllotmentDistributorPageContent";
}



@*Main Content*@
<div class="content-wrapper">
    <div class="container-fluid">

        <div class="row pt-2 pb-2">

            <div class="col-sm-3">
                <div class="btn-group float-sm-right">

                    <div class="dropdown-menu">
                        <a href="javaScript:void();" class="dropdown-item">Action</a>
                        <a href="javaScript:void();" class="dropdown-item">Another action</a>
                        <a href="javaScript:void();" class="dropdown-item">Something else here</a>
                        <div class="dropdown-divider"></div>
                        <a href="javaScript:void();" class="dropdown-item">Separated link</a>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Breadcrumb-->





        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-header"><i class="fa fa-table"></i>Document Allotment Data</div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table id="default-datatable" class="table table-bordered">
                                <thead>
                                    <tr class="ui-widget-header">
                                        <th>Approval Date</th>
                                        <th>Approved Count</th>
                                        <th>Balanced Count</th>
                                        <th>Document Type</th>
                                        <th>Status</th>


                                    </tr>
                                </thead>
                                <tbody id="AllotData"></tbody>
                            </table>

                        </div>
                    </div>
                </div>
            </div>
        </div><!-- End Row-->

        <input type="button" style="display:none" id="btndeleteroles" name="name" value="" />

    </div>


</div>









@*end*@





<script>

    $(document).ready(function () {

        $.ajax({

            type: "POST",
            url:"@Url.Action("BindDistributorAllotment", "EditAllotmentDistributor")",
            success: function (data) {
                $('#AllotData').empty().append(data);
            }
        });


    });




</script>




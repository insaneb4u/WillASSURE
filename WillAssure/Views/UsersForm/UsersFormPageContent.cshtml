<script>

    $('.radio').on('change', function () {
       
        $('.radio').not(this).prop('checked', false);
        var val = $("input[name=radio]:checked").val()
      
        if (val == "Yes") {
          
            $("#showcompany").removeAttr("style");
            $(".companyclass").removeAttr("style");
            $("#txtcheck").val("true");

        }

        if (val == "No") {
          
            $(".companyclass").css("display", "none");
            $("#txtcheck").val("false");
         
        }


    });


    $(document).ready(function () {

        $.ajax({

            type: "POST",
            url:"@Url.Action("BindRoleDDL", "UsersForm")",
            success: function (data) {
                $('#ddlRole').empty().append(data);
            }
        });




              $.ajax({

            type: "POST",
            url:"@Url.Action("BindCompanyDDL", "UsersForm")",
            success: function (data) {
                $('#ddlcompany').empty().append(data);
            }
        });


    });


</script>


        var txtfirstname = $("#txtfirstname").val();
        var txtmiddlename = $("#txtmiddlename").val();
        var txtlastname = $("#txtlastname").val();
        var DOB = $("#DOB").val();
        var txtmobile = $("#txtmobile").val();
        var RelationshipTxt = $("#RelationshipTxt").val();
        var nommaritalstatus = $("#nommaritalstatus").val();
        var nomreligion = $("#nomreligion").val();
        var statetext = $("#statetext").val();
        var citytext = $("#citytext").val();
        var txtpin = $("#txtpin").val();
        var txtaddress1 = $("#txtaddress1").val();
        var txtaddress2 = $("#txtaddress2").val();
        var txtaddress3 = $("#txtaddress3").val();
        var identityproof = $("#identityproof").val();
        var txtidentityproof = $("#Identity_Proof_Value").val();
        var Alt_Identity_Proof = $("#Alt_Identity_Proof").val();
        var txtIdentityProofValue = $("#Alt_Identity_proof_Value").val();
        var txtdescriptionofasset = $("#txtdescriptionofasset").val();

        var data = txtfirstname + "~" + txtmiddlename + "~" + txtlastname + "~" + DOB + "~" + txtmobile + "~" + RelationshipTxt + "~" + nommaritalstatus + "~" + nomreligion + "~" + statetext + "~" + citytext + "~" + txtpin + "~" + txtaddress1 + "~" + txtaddress2 + "~" + txtaddress3 + "~" + identityproof + "~" + txtidentityproof + "~" + Alt_Identity_Proof + "~" + txtIdentityProofValue + "~" + txtdescriptionofasset;



        $.ajax({

            type: "POST",
            url: "@Url.Action("InsertNomineeData", "AddMainAssets")",
